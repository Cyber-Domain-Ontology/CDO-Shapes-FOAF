# imports: http://xmlns.com/foaf/0.1/
# imports: https://ontology.unifiedcyberontology.org/owl/1.2.0

@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix sh-foaf: <http://example.org/shapes/foaf/> .
@prefix uco-owl: <https://ontology.unifiedcyberontology.org/owl/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/shapes/foaf>
	a owl:Ontology ;
	owl:imports
		<http://xmlns.com/foaf/0.1/> ,
		uco-owl:1.2.0
		;
	.

sh-foaf:depiction-range-shape
	a sh:NodeShape ;
	sh:class foaf:Image ;
	sh:nodeKind sh:BlankNodeOrIRI ;
	sh:targetObjectsOf foaf:depiction ;
	.

sh-foaf:depicts-domain-shape
	a sh:NodeShape ;
	sh:class foaf:Image ;
	sh:targetSubjectsOf foaf:depicts ;
	.

sh-foaf:depicts-range-shape
	a sh:NodeShape ;
	sh:nodeKind sh:BlankNodeOrIRI ;
	sh:targetObjectsOf foaf:depicts ;
	.

